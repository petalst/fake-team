{
    "name": "petalst/fake-team",
    "description": "Create fake team data",
    "minimum-stability": "stable",
    "keywords": [
        "faker",
        "team",
        "fake-team"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Petter Alstad",
            "email": "petter.alstad@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3||^8.0",
        "fakerphp/faker": "^1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Faker\\Provider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faker\\Tests\\Provider\\": "tests/Faker"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit -c phpunit.xml"
        ]
    }
}
